[
  {xabber_ws,
    [
      {ws_path, "/websocket"},
      {client_path, "/client"},
      {port, 8080},
      {ws_timeout, 300000},
      {dns_timeout, 3000},
      {dns_retries, 3}
    ]
  },
  {lager, [
    {log_root, "log"},
    {error_logger_hwm, 123},
    {handlers, [
      {lager_console_backend, [{level, info}]},
      {lager_file_backend, [{file, "error.log"}, {level, error}, {size, 10485760}, {date, "$D0"}, {count, 5}]},
      {lager_file_backend, [{file, "xabber_ws.log"}, {level, info}, {size, 10485760}, {date, "$D0"}, {count, 5}]}
    ]}
  ]}
].